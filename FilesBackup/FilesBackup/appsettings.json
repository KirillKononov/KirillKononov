{
  "TargetPath": "C:\\TargetFolder",
  "SourcePaths": [
    "C:\\SourceFolder",
    "C:\\SourceFolder2"
  ],
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Log-.txt",
          "rollingInterval": "Minute",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  }
}

